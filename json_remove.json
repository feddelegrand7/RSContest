{
    "dataSource": {
        "dataSourceType": "csv",
        "filename": "https://raw.githubusercontent.com/rfordatascience/tidytuesday/master/data/2020/2020-03-17/office_ratings.csv",
        "fieldSeparator": ","
    },
    "slice": {
        "rows": [
            {
                "uniqueName": "title"
            },
            {
                "uniqueName": "air_date.Year"
            },
            {
                "uniqueName": "air_date.Month"
            },
            {
                "uniqueName": "air_date.Day"
            }
        ],
        "columns": [
            {
                "uniqueName": "Measures"
            }
        ],
        "measures": [
            {
                "uniqueName": "season",
                "aggregation": "sum"
            },
            {
                "uniqueName": "episode",
                "aggregation": "sum"
            },
            {
                "uniqueName": "imdb_rating",
                "aggregation": "sum"
            },
            {
                "uniqueName": "total_votes",
                "aggregation": "sum"
            }
        ],
        "flatOrder": [
            "season",
            "episode",
            "title",
            "imdb_rating",
            "total_votes",
            "air_date.Year",
            "air_date.Month",
            "air_date.Day"
        ]
    },
    "options": {
        "grid": {
            "type": "flat",
            "showTotals": "off",
            "showGrandTotals": "off"
        }
    }
}